{
  "manifest_version": 3,
  "name": "Tree-Style Tabs",
  "version": "1.0",
  "description": "Organize tabs hierarchically in a tree structure.",
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "sidebar/sidebar.html",
    "default_icon": {
      "16": "icons/icon16.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png"
  }
}
